<?xml version="1.0" encoding="UTF-8" standalone="yes"?><ruleSet xmlns="http://caaers.cabig.nci.nih.gov/rules/brxml"><id>528c69d0-6622-47a4-9bf2-b635d844d1b5</id><name>gov.nih.nci.cabig.caaers.rules.sponsor.division_of_cancer_prevention.sae_reporting_rules</name><description>SAE Reporting Rules</description><subject>Sponsor rules||Division of Cancer Prevention</subject><coverage>Enabled</coverage><import>gov.nih.nci.cabig.caaers.domain.*</import><rule id="38810e17-b0bb-4585-9ea8-5e69fb790cc7"><condition><column display-uri="Study" object-type="gov.nih.nci.cabig.caaers.domain.Study" identifier="study"><field-constraint grammer-postfix="" grammer-prefix=" AdEERS Reporting " display-uri="undefined" field-name="adeersReporting"><literal-restriction display-uri="is " evaluator="=="><value>false</value><readable-value>No </readable-value></literal-restriction></field-constraint><expression>factResolver.assertFact(study,null,'adeersReporting','runTimeValue','runTimeOperator')</expression></column><column display-uri="Study" object-type="gov.nih.nci.cabig.caaers.domain.Study" identifier="study"><field-constraint grammer-postfix="" grammer-prefix=" Study " display-uri="undefined" field-name="phaseCode"><literal-restriction display-uri="is " evaluator="=="><value>Phase I Trial</value><value>Phase I/II Trial</value><value>Phase II Trial</value><readable-value>Phase II Trial,Phase I/II Trial,Phase I Trial,</readable-value></literal-restriction></field-constraint><expression>factResolver.assertFact(study,null,'phaseCode','runTimeValue','runTimeOperator')</expression></column><column display-uri="Expedited Adverse Event Report" object-type="gov.nih.nci.cabig.caaers.domain.ExpeditedAdverseEventReport" identifier="aeReport"><field-constraint grammer-postfix="" grammer-prefix=" Outcome Identifier " display-uri="undefined" field-name="outcomeIdentifier"><literal-restriction display-uri="is " evaluator="=="><value>1</value><value>2</value><value>3</value><value>4</value><value>5</value